<Window x:Class="KineCon.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="KineCon" Height="480" Width="640"
        WindowState="Maximized"
        >
    <Grid>
        <Image x:Name="ImageColor" Stretch="UniformToFill" />
        <TextBlock x:Name="TextMessage" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="50" />
    </Grid>
</Window>
